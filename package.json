{
  "name": "makenew-jekyll-site",
  "version": "1.3.1",
  "description": "Demo site for Make New's Jekyll site skeleton.",
  "main": "index.html",
  "private": true,
  "keywords": [
    "jekyll"
  ],
  "homepage": "https://github.com/makenew/jekyll-site",
  "bugs": "https://github.com/makenew/jekyll-site/issues",
  "repository": "makenew/jekyll-site",
  "license": "MIT",
  "author": {
    "name": "Evan Sosenko",
    "email": "razorx@evansosenko.com"
  },
  "scripts": {
    "install": "npm run bower",
    "postinstall": "npm run modernizr && npm run vulcanize",
    "bower": "bower install",
    "deploy": "gulp deploy",
    "lint": "gulp lint",
    "lint:watch": "gulp watch",
    "optimize": "gulp modernizr && gulp hash && gulp minify",
    "modernizr": "modernizr -c modernizr-config.json -d src/_assets/javascripts/vendor/modernizr.js",
    "vulcanize": "vulcanize src/_assets/elements/elements.html > src/_assets/elements/vulcanized.html"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "modernizr": "~3.3.1"
  },
  "devDependencies": {
    "bower": "^1.7.4",
    "eslint": "^2.2.0",
    "git-rev-sync": "^1.4.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^1.3.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-modernizr": "^1.0.0-alpha",
    "gulp-plumber": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-sass-lint": "^1.1.1",
    "gulp-standard": "^6.0.5",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "vulcanize": "^1.14.6"
  }
}
